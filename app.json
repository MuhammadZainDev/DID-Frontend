{
  "expo": {
    "name": "Duaon AI",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "duaonai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212",
      "imageWidth": 120,
      "imageHeight": 120,
      "androidSplashResourceName": "splash",
      "androidStatusBar": {
        "backgroundColor": "#121212",
        "translucent": false
      },
      "mdpi": true,
      "hdpi": true,
      "xhdpi": true
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show accurate prayer times for your area.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to show accurate prayer times for your area."
      },
      "bundleIdentifier": "com.muhammadzainraza.duaonai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.muhammadzainraza.duaonai",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize",
      "windowTranslucentStatus": false,
      "windowTranslucentNavigation": false,
      "splash": {
        "image": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212",
        "resizeMode": "contain",
        "imageWidth": 120,
        "imageHeight": 120
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Duaon AI to use your location to show accurate prayer times for your area."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9ea25fd9-9009-468d-9828-54ccbc1b91b3"
      }
    }
  }
}
